<div class="container mt-5">
  <div class="row justify-content-center">
    <div class="col-4">
      <h1>New List</h1>
      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.submit "Submit list", class: "btn btn-primary mb-3" %>
      <% end %>
      <%= link_to "Go back", lists_path %>
    </div>
  </div>
</div>
